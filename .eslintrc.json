{
  "extends": "google",
  "parserOptions": {
    "ecmaVersion": 5,
    "sourceType": "script",
    "ecmaFeatures": {
      "jsx": false
    }
  },
  "rules": {
    "linebreak-style": "off",
    "camelcase": "off",
    "no-var": "off",
    "no-extend-native": "off",
    "no-unused-vars": "warn",
    "guard-for-in": "warn",
    "max-len": "warn",
    "no-extend-native": "warn"
  }
}
